{
  "manifest_version": 2,
  "name": "API Test tool Chrome Extension @ yuzc",
  "version": "0.0.1",
  "version_name": "alpha",
  "description": "just for lieluobo.com @ http://yoda:9001.  ",
  "devtools_page": "html/devtool.html",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "20": "images/favicon-32x32.png",
      "32": "images/favicon-32x32.png"
    },
    "default_title": "MarkTable",
    "default_popup": "html/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://yoda:9001/server/*","http://*.lieluobo.testing/*"],
      "js": [
        "lib/jquery-3.2.1.min.js",
        "src/content.js",
        "lib/jquery-ui.min.js"
      ],
      "run_at":"document_end"
    }
  ],
  "author": "youcxx@gmail.com",
  "homepage_url": "https://github.com/youwi/api-test-tool-chrome-helper",
  "incognito": "spanning",
  "minimum_chrome_version": "46",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "src/background.js"
    ]
  },


  "short_name": "starter"
}


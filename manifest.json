{
  "manifest_version": 2,
  "name": "umock-mark-extension",
  "version": "0.0.1",
  "version_name": "alpha",
  "description": "just for umock.com",
  "devtools_page": "src/devtool.html",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "20": "images/icon20.png",
      "32": "images/icon32.png"
    },
    "default_title": "MarkTable",
    "default_popup": "dist/index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/jquery-3.2.1.min.js","src/content.js"],
      "run_at":"document_end"
    }
  ],
  "author": "youcxx@gmail.com",
  "homepage_url": "https://github.com/youwi/chrome-extension-umock",
  "incognito": "spanning",
  "minimum_chrome_version": "46",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "src/background.js"
    ]
  },


  "short_name": "starter"
}

